<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF">

    <ScrollView
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/card_img"
        app:layout_constraintVertical_bias="0.366">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TableLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TableRow
                    android:id="@+id/day_tr"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:id="@+id/day_img"
                        android:layout_width="88dp"
                        android:layout_height="88dp"
                        app:srcCompat="@drawable/event" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/day_txt"
                            android:layout_width="match_parent"
                            android:layout_height="55dp"
                            android:gravity="bottom|center_horizontal"
                            android:text="TextView"
                            android:textSize="22dp" />

                        <ProgressBar
                            android:id="@+id/day_pb"
                            style="?android:attr/progressBarStyleHorizontal"
                            android:layout_width="281dp"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="10dp" />
                    </LinearLayout>

                </TableRow>

                <TableRow
                    android:id="@+id/level_tr"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:id="@+id/level_img"
                        android:layout_width="88dp"
                        android:layout_height="88dp"
                        app:srcCompat="@drawable/event" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/level_txt"
                            android:layout_width="match_parent"
                            android:layout_height="55dp"
                            android:gravity="bottom|center_horizontal"
                            android:text="TextView"
                            android:textSize="22dp" />

                        <ProgressBar
                            android:id="@+id/level_pb"
                            style="?android:attr/progressBarStyleHorizontal"
                            android:layout_width="281dp"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="10dp" />
                    </LinearLayout>

                </TableRow>

                <TableRow
                    android:id="@+id/distance_tr"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:id="@+id/distance_img"
                        android:layout_width="88dp"
                        android:layout_height="88dp"
                        app:srcCompat="@drawable/event" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/distance_txt"
                            android:layout_width="match_parent"
                            android:layout_height="55dp"
                            android:gravity="bottom|center_horizontal"
                            android:text="TextView"
                            android:textSize="22dp" />

                        <ProgressBar
                            android:id="@+id/distance_pb"
                            style="?android:attr/progressBarStyleHorizontal"
                            android:layout_width="281dp"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="10dp" />
                    </LinearLayout>

                </TableRow>

                <TableRow
                    android:id="@+id/step_tr"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:id="@+id/step"
                        android:layout_width="88dp"
                        android:layout_height="88dp"
                        app:srcCompat="@drawable/event" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/step_txt"
                            android:layout_width="match_parent"
                            android:layout_height="55dp"
                            android:gravity="bottom|center_horizontal"
                            android:text="TextView"
                            android:textSize="22dp" />

                        <ProgressBar
                            android:id="@+id/step_pb"
                            style="?android:attr/progressBarStyleHorizontal"
                            android:layout_width="281dp"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="10dp" />
                    </LinearLayout>

                </TableRow>

                <TableRow
                    android:id="@+id/development_tr"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:id="@+id/development_img"
                        android:layout_width="88dp"
                        android:layout_height="88dp"
                        app:srcCompat="@drawable/event" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/development_txt"
                            android:layout_width="match_parent"
                            android:layout_height="55dp"
                            android:gravity="bottom|center_horizontal"
                            android:text="TextView"
                            android:textSize="22dp" />

                        <ProgressBar
                            android:id="@+id/development_pb"
                            style="?android:attr/progressBarStyleHorizontal"
                            android:layout_width="281dp"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="10dp" />
                    </LinearLayout>

                </TableRow>

                <TableRow
                    android:id="@+id/calorie_tr"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:id="@+id/calorie_img"
                        android:layout_width="88dp"
                        android:layout_height="88dp"
                        app:srcCompat="@drawable/event" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/calorie_txt"
                            android:layout_width="match_parent"
                            android:layout_height="55dp"
                            android:gravity="bottom|center_horizontal"
                            android:text="TextView"
                            android:textSize="22dp" />

                        <ProgressBar
                            android:id="@+id/calorie_pb"
                            style="?android:attr/progressBarStyleHorizontal"
                            android:layout_width="281dp"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="10dp" />
                    </LinearLayout>

                </TableRow>

                <TableRow
                    android:id="@+id/friend_tr"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:id="@+id/friend_img"
                        android:layout_width="88dp"
                        android:layout_height="88dp"
                        app:srcCompat="@drawable/event" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/friend_txt"
                            android:layout_width="match_parent"
                            android:layout_height="55dp"
                            android:gravity="bottom|center_horizontal"
                            android:text="TextView"
                            android:textSize="22dp" />

                        <ProgressBar
                            android:id="@+id/friend_pb"
                            style="?android:attr/progressBarStyleHorizontal"
                            android:layout_width="281dp"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="10dp" />
                    </LinearLayout>

                </TableRow>

                <TableRow
                    android:id="@+id/event_tr"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:id="@+id/event_img"
                        android:layout_width="88dp"
                        android:layout_height="88dp"
                        app:srcCompat="@drawable/event" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/event_txt"
                            android:layout_width="match_parent"
                            android:layout_height="55dp"
                            android:gravity="bottom|center_horizontal"
                            android:text="TextView8"
                            android:textSize="22dp" />

                        <ProgressBar
                            android:id="@+id/event_pb"
                            style="?android:attr/progressBarStyleHorizontal"
                            android:layout_width="281dp"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="10dp" />
                    </LinearLayout>

                </TableRow>

            </TableLayout>
        </LinearLayout>
    </ScrollView>

    <Button
        android:id="@+id/edit"
        android:layout_width="wrap_content"
        android:layout_height="48dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="649dp"
        android:text="編集"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.95"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/card_img"
        android:layout_width="243dp"
        android:layout_height="122dp"
        android:layout_marginTop="24dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:srcCompat="@tools:sample/avatars" />

    <FrameLayout
        android:id="@+id/back_fl"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </FrameLayout>

    <FrameLayout
        android:id="@+id/frame"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="32dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>
<!-- バッジ一覧（並び順変更済み)
バッジ名:B,S,G,P
ログイン日数 :7,30,180,365
            :30,100,500,1000
レベル:10,50,100,150
      :10,25,50,100
総移動距離(km):10,100,1000,10000
総歩数(万):10,100,500,1000
新規開拓距離:1,10,100,500
消費カロリー:1000,5000,25000,50000
すれ違い人数:1,10,50,100
イベント参加回数:1,3,6,10
-->